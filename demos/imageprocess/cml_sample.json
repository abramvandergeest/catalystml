{
  "name":"ImageProccessing",
  "description":"Converts image to data",
  "version":"0.0.1",
  "createdDate":"20190709",
  "model":{
      "framework":"Tensorflow",
      "tags":"serve",
      "signatureDefs":"def_signature"
  },
"input": [
  {
      "type": "image",
      "label":"img"
  }
],
"structure": [
                {

                  "operation":"resize",
                  "input":{"img":"$img"},  
                  "params":{"xsize":256,"ysize":256},               
                  "output":"rs"
                },
                {
                  "operation":"img2tensor",
                  "input":{"img":"$rs"},
                  "params":{
                    "removeAlpha":true,
                    "includeBatch":true
                  },
                  "output":"tensor"
                },
                {
                  "operation":"normalize",
                  "input":
                  {
                    "data":"$tensor",
                    "value":256
                  },
                  "output":"normalized"
                }
           ],
"output": {
  "type": "array",
  "data":"$normalized"
}
}
